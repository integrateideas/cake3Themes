{
    "name": "integrateideas/InspiniaTheme",
    "description": "InspiniaTheme plugin for CakePHP",
    "type": "cakephp-plugin",
    "homepage": "https://github.com/integrateideas/cake3Themes/InspiniaTheme",
    "authors": [
       {
           "name":"Integrateideas",
           "role":"Author",
           "homepage":"https://github.com/integrateideas"
       }
    ],
    "license": "MIT",
    "repositories": [
       {
           "type": "vcs",
           "url": "https://github.com/integrateideas/cake3Themes/InspiniaTheme"
       } 
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.4 <4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "InspiniaTheme\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InspiniaTheme\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
